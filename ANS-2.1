import java.util.*;
 
public class acad {         
 
public static void main(String[] args) {
 
Scanner sc=new Scanner(System.in);
 
int n=sc.nextInt();
 
int[] a=new int[n];
 
int[] b=new int[n];
 
for(int i=0;i<n;i++)
 
{
 
a[i]=sc.nextInt();
 
}
 
Arrays.sort(a);
 
for(int i=0;i<n;i++)
 
{ 
 
b[i]=a[n-i-1];
 
}
 
for(int i=0;i<n;i++)
 
{
 
System.out.println(b[i]);
 
}
 
}
